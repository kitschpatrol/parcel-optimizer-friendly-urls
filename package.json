{
  "name": "@kitschpatrol/parcel-optimizer-friendly-urls",
  "version": "0.2.0",
  "description": "Remove index.html from internal links in Parcel 2.x build output",
  "keywords": [
    "parcel",
    "parcel-optimizer",
    "parcel-plugin",
    "friendly-urls",
    "index-html"
  ],
  "author": "Mark van Seventer <mark@vseventer.com> (https://www.vseventer.com)",
  "repository": "kitschpatrol/parcel-optimizer-friendly-urls",
